{
  "venvPath": "/home/ben/.pyenv/versions/",
  "venv": "darwin",
  "pythonVersion": "3.10",
  "pythonPlatform": "Linux",
  "disableOrganizeImports": true,
  "analysis": {
    "autoImportCompletions": true,
    "diagnosticMode": "workspace",
    "logLevel": "Warning",
    "typeCheckingMode": "strict",
    "useLibraryCodeForTypes": true,
    "stubPath": [
      ".stubs"
    ]
  },
  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    "src/experimental"
  ]
}